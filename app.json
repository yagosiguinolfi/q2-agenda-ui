{
  "expo": {
    "name": "q2-agenda-ui",
    "slug": "q2-agenda-ui",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo-azul.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash-azul.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./src/assets/logo-azul.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo-azul.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
